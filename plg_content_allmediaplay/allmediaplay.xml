                <option value="7.0.24">7.0.24</option>
                <option value="7.0.19">7.0.19</option>
                <option value="7.0.14">7.0.14</option>
            </field> 
            <field name="pbgcolor" type="color" default="#FFFFFF" size="8" label="LBL_PBGCOLOR" description="DSC_PBGCOLOR" />
            <field name="pfgcolor" type="color" default="#000000" size="8" label="LBL_PFGCOLOR" description="DSC_PFGCOLOR" />
            <field name="phicolor" type="color" default="#000000" size="8" label="LBL_PHICOLOR" description="DSC_PHICOLOR" />
            <field name="psccolor" type="color" default="#000000" size="8" label="LBL_PSCCOLOR" description="DSC_PSCCOLOR" />
            <field name="autostart" type="list" default="0" label="LBL_AUTOSTART" description="DSC_AUTOSTART">
                <option value="0">OPT_NO</option>
                <option value="1">OPT_YES</option>
            </field>
            <field name="shownav" type="list" default="1" label="LBL_SHOWNAV" description="DSC_SHOWNAV">
                <option value="0">OPT_NO</option>
                <option value="1">OPT_YES</option>
            </field>
            <field name="showdigits" type="list" default="1" label="LBL_SHOWDIGITS" description="DSC_SHOWDIGITS">
                <option value="0">OPT_NO</option>
                <option value="1">OPT_YES</option>
            </field>
            <field name="showicons" type="list" default="1" label="LBL_SHOWICONS" description="DSC_SHOWICONS">
                <option value="0">OPT_NO</option>
                <option value="1">OPT_YES</option>
            </field>
            <field name="showstop" type="list" default="0" label="LBL_SHOWSTOP" description="DSC_SHOWSTOP">
                <option value="0">OPT_NO</option>
                <option value="1">OPT_YES</option>
            </field>
            <field name="showdownload" type="list" default="0" label="LBL_SHOWDL" description="DSC_SHOWDL">
                <option value="0">OPT_NO</option>
                <option value="1">OPT_YES</option>
            </field>
            <field name="usefullscreen" type="list" default="0" label="LBL_USEFULLSCREEN" description="DSC_USEFULLSCREEN">
                <option value="0">OPT_NO</option>
                <option value="1">OPT_YES</option>
            </field>
            <field name="searchbar" type="list" default="0" label="LBL_SHOWSEARCHBAR" description="DSC_SHOWSEARCHBAR">
                <option value="0">OPT_NO</option>
                <option value="1">OPT_YES</option>
            </field>
            <field name="showeq" type="list" default="0" label="LBL_SHOWEQ" description="DSC_SHOWEQ">
                <option value="0">OPT_NO</option>
                <option value="1">OPT_YES</option>
            </field>
            <field name="enablejs" type="list" default="0" label="LBL_ENABLEJS" description="DSC_ENABLEJS">
                <option value="0">OPT_NO</option>
                <option value="1">OPT_YES</option>
            </field>
            <field name="menu" type="list" default="1" label="LBL_MENU" description="DSC_MENU">
                <option value="0">OPT_NO</option>
                <option value="1">OPT_YES</option>
            </field>
            <field name="searchlink" type="text" default="" size="40" label="LBL_SEARCHLINK" description="DSC_SEARCHLINK" />
            <field name="logo" type="imagelist" directory="/images" filter="" hide_default="1" default="" label="LBL_LOGO" description="DSC_LOGO" />
            <field name="screenmode" type="list" default="default" label="LBL_SCREENMODE" description="DSC_SCREENMODE">
                <option value="default">OPT_DEFAULT</option>
                <option value="coverlay">OPT_FLOATCONTROL</option>
                <option value="plbottom">OPT_PLBOTTOM</option>
                <option value="plright">OPT_PLRIGHT</option>
                <option value="floatcplright">OPT_FLOATCPLRIGHT</option>
            </field>
            <field name="plsize" type="text" default="30" size="5" label="LBL_PLSIZE" description="DSC_PLSIZE" />
            <field name="stretch" type="list" default="0" label="LBL_STRETCH" description="DSC_STRETCH">
                <option value="0">OPT_STRETCH_FALSE</option>
                <option value="1">OPT_STRETCH_TRUE</option>
                <option value="2">OPT_STRETCH_FIT</option>
                <option value="3">OPT_STRETCH_NONE</option>
            </field>
            <field name="alt" type="textarea" rows="3" cols="40" default="" label="LBL_ALTCONTENT" description="DSC_ALTCONTENT" />
            <!--<field name="" type="header" default="LBL_KOMPPARAMETER" label="" description="" />-->
            <field name="legacy" type="list" default="0" label="LBL_LEGACY" description="DSC_LEGACY">
                <option value="0">OPT_NO</option>
                <option value="1">OPT_YES</option>
            </field>
            <field name="tmargin" type="text" default="8" size="5" label="LBL_TOPMARGIN" description="DSC_TOPMARGIN" />
            <field name="bmargin" type="text" default="8" size="5" label="LBL_BOTMARGIN" description="DSC_BOTMARGIN" />
            <field name="valign" type="list" default="center" label="LBL_ALIGN" description="DSC_ALIGN">
                <option value="center">OPT_CENTER</option>
                <option value="left">OPT_LEFT</option>
                <option value="right">OPT_RIGHT</option>
            </field>
        </fieldset>
    </fields>
</config>
       
